<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:left="1dp"
        android:bottom="1dp"
        android:top="1dp"
        android:end="1dp"
        android:start="0dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="#A1A1A1" />
            <solid android:color="#009BCE64"  />
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item  android:bottom="0dp" android:top="0dp"
        android:end="0dp" android:start="0dp" >
        <shape android:shape="rectangle" >
            <stroke android:width="2dp" android:color="#009BCE64" />
            <solid android:color="#00030925"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>

